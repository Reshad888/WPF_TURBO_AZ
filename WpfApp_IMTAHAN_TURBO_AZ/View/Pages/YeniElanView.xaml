                        Content="Əlaqə"
                        HorizontalAlignment="Left"
                        HorizontalContentAlignment="Left"
                        FontSize="20"
                        FontWeight="DemiBold"
                            
                        />
                        <TextBlock Margin="5 0 0 0" Foreground="DarkGray" Text="Elan dərc olunduqdan sonra əlaqə məlumatları ilə bağlı heç bir dəyişiklik həyata keçirilmir."></TextBlock>

                        <Grid Margin="0 10 0 0" Height="25">
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="150"/>
                                <ColumnDefinition Width="3*"/>
                                
                            </Grid.ColumnDefinitions>

                            <Label Grid.Column="0"
                           Content="Adınız"
                           HorizontalAlignment="Left"
                           HorizontalContentAlignment="Left"
                           />
                            <ComboBox 
                            
                            Grid.Column="1"
                            
                            Name="ElaqeAdComboBox"
                            Height="25" 
                            Width="235" 
                            IsEditable="True"
                                
                            Style="{StaticResource MaterialDesignFilledComboBox}"
                            HorizontalAlignment="Left"
                               
